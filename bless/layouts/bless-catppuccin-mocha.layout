<layout>
    <area type="offset">
	<display>
	    <evenrow>
		<evencolumn>
		    <foreground>#f38ba8</foreground>
		    <background>#1e1e2e</background>
		</evencolumn>
	    </evenrow>
	    <oddrow>
		<evencolumn>
		    <foreground>#f38ba8</foreground>
		    <background>#1e1e2e</background>
		</evencolumn>
	    </oddrow>
	</display>
    </area>

    <area type="separator">
